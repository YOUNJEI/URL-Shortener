{{>layout/header}}

<h1>Visitor details</h1>
<div class="col-md-12">
    <div class="row">
        <div class="col-md-6">
            <a href="/page/url-list" class="btn btn-outline-info" role="button">Back</a>
        </div>
    </div>
</div>
<br>
<table class="table table-hover">
    <thead class="thead-strong"><h5>{{response.origin}} ({{response.shortUrl}})</h5></thead>
    <tr>
        <th>id</th>
        <th>browser</th>
        <th>language</th>
        <th>location</th>
        <th>visited</th>
    </tr>
    <tbody id="tbody">
{{#response.visitors}}
    <tr>
        <td>{{id}}</td>
        <td>{{browser}}</td>
        <td>{{language}}</td>
        <td>{{location}}</td>
        <td>{{visited}}</td>
    </tr>
{{/response.visitors}}
    </tbody>
</table>
{{>layout/footer}}